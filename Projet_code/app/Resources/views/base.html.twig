        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        
        <div id="page">
            <div id="header">

                <div class="navbar">
                    <div class="navbar-inner">
                        <a class="brand" href="{{ path('homepage') }}">Accueil</a>
                        <a class="brand" href="{{ path('client_reservation_volsdisponibles') }}">Partie Client</a>
                        <a class="brand" href="{{ path('admin_vol_index') }}">Partie Administration</a>
                    </div>
                </div>
            </div>

            <div id="content">
              <h1>{{ block('title') }}</h1>
              {% block content %}
              {% endblock %}
            </div>
            <div id="footer">
              </br>
              <p>Libre de tous droits 2014</p>
            </div>
        </div>
    </body>
</html>
